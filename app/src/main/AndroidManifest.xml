<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.corresponsal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Corresponsal">
        <activity android:name=".BankActivity"
            android:label="Banco"
            android:parentActivityName=".ListBanksActivity"/>
        <activity
            android:name=".ProfileActivity"
            android:label="Perfil"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HistoricalActivity"
            android:label="Historical"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ListBanksActivity"
            android:label="List Banks"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>